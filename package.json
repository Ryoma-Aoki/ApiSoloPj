{
   "name": "apisolopj",
   "version": "0.0.0",
   "private": true,
   "scripts": {
      "start": "nodemon src/index.ts",
      "typeorm-cli": "typeorm",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "testConnection": "ts-node -r dotenv/config src/testConnection.ts",
      "dev": "node --inspect -r ts-node/register -r dotenv/config src/index.ts",
      "build": "tsc -p .",
      "lint": "eslint ./src/**/*.ts --fix",
      "test": "mocha --inspect -r ts-node/register -r dotenv/config tests/index.ts",
      "orm": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js --config ormconfig.ts",
      "makeMigrations": "yarn orm migration:generate",
      "migrate": "yarn orm migration:run",
      "rollback": "echo 'FIXME!'",
      "seeder": "ts-node -r dotenv/config ./node_modules/typeorm-seeding/dist/cli.js",
      "seed": "yarn seeder --configName ormconfig.ts seed",
      "pre-commit": "yarn lint",
      "seed-run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
   },
   "dependencies": {
      "@types/chai": "^4.2.11",
      "@types/chai-http": "^4.2.0",
      "@types/lodash.pick": "^4.4.6",
      "@types/mocha": "^7.0.2",
      "@types/pg": "^7.14.3",
      "@types/uuid": "^7.0.2",
      "bcrypt": "4.0",
      "body-parser": "^1.18.1",
      "chai": "^4.2.0",
      "chai-http": "^4.3.0",
      "cookie-parser": "~1.4.4",
      "cors": "^2.8.5",
      "debug": "~2.6.9",
      "dotenv": "^8.2.0",
      "express": "^4.15.4",
      "http-errors": "~1.6.3",
      "husky": "^4.2.3",
      "jsonwebtoken": "^8.5.1",
      "lodash.pick": "^4.4.0",
      "mocha": "^7.1.1",
      "morgan": "~1.9.1",
      "nodemon": "^2.0.6",
      "pg": "^8.4.0",
      "prettier": "^2.0.2",
      "pug": "2.0.0-beta11",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.2.29",
      "typeorm-seeding": "^1.6.1",
      "uuid": "^7.0.2"
   },
   "devDependencies": {
      "@types/node": "^8.0.29",
      "ts-node": "^9.0.0",
      "typescript": "^4.0.5"
   }
}
